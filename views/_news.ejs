<div class="form-group row">
  <label class="col-form-label col-sm-2" for="company_name">Company Name</label>
  <div class="col-sm-7">
    <input readonly class="form-control" name="company_name" value="<%= model.company_name %>" />
  </div>
</div>


<div class="form-group row">
  <label class="col-form-label col-sm-2" for="website">Website</label>
  <div class="col-sm-10">
    <input readonly class="form-control" name="website" value="<%= model.website %>" />
  </div>
</div>

<div class="form-group row">
  <label class="col-form-label col-sm-2"></label>
  <div class="col-sm-10">
    <a class="btn btn-outline-dark cancel" href="/uraniumstocks">Go Back to Stock Lists</a>
  </div>
</div>

<div class="container">
  <br>
  <h2>Youtube Search</h2>
  <br>

  <form id="form">
    <div class="form-group">
      <input type="text" class="form-control" name="search_value" id="search" value="<%= model.company_name %>">
    </div>
    <br>
    <div class="form-group">
      <input type="submit" class="btn btn-danger" value="Search">
    </div>

  </form>
</div>
<br>

<div class="row">
  <div class="col-md-12">
    <div id="videos">
      
    </div>
  </div>
</div>



